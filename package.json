{
  "name": "sveltekit-graphql-fetch",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev -p 3030",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -p 3030",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "gen": "export $(grep SWOP_API_KEY .env | xargs) && graphql-codegen --config codegen.json && prettier --write src/lib/generated/graphql.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@fontsource/source-sans-pro": "^4.5.1",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@sveltejs/adapter-netlify": "1.0.0-next.32",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "graphql-codegen-svelte-apollo": "^1.0.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.1",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-grapqhql-fetch"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-graphql-fetch/issues"
  },
  "type": "module"
}
